<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>servletLogin</servlet-name>
        <servlet-class>atencion.servlets.servletLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletLog</servlet-name>
        <servlet-class>atencion.servlets.ServletLog</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletRegistrarEntrevista</servlet-name>
        <servlet-class>atencion.servlets.servletRegistrarEntrevista</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletEliminarPostulante</servlet-name>
        <servlet-class>atencion.servlets.servletEliminarPostulante</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletModificarPostulante</servlet-name>
        <servlet-class>atencion.servlets.servletModificarPostulante</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletListarPostulante</servlet-name>
        <servlet-class>atencion.servlets.servletListarPostulante</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servletLogin</servlet-name>
        <url-pattern>/loginUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletLog</servlet-name>
        <url-pattern>/ServletLog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletRegistrarEntrevista</servlet-name>
        <url-pattern>/registrar-entevista</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletEliminarPostulante</servlet-name>
        <url-pattern>/eliminarPostulante</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletModificarPostulante</servlet-name>
        <url-pattern>/modificarPostulante</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletListarPostulante</servlet-name>
        <url-pattern>/listarPostulante</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
